{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/api/main.py"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/backend/api/main.py"
    },
    {
      "source": "/docs",
      "destination": "/backend/api/main.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/backend/api/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/index.html"
    }
  ]
}